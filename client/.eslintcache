Router exact path='/register' component={Register} isAuth={state.user.isAuth} />\n                <ProtectedAuthRouter path='/reset/:token' component={Reset} isAuth={state.user.isAuth} />\n                <ProtectedRouter exact path='/user' component={User} isAuth={state.user.isAuth}/>\n                <ProtectedRouter exact path='/user/change' component={UserChange} isAuth={state.user.isAuth}/>\n                <ProtectedRouter exact path='/article' component={ArticleDetails} isAuth={state.user.isAuth} />\n            </div>\n        </div>\n    );\n}\n\nexport default App\n","C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\reportWebVitals.js",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\Register\\Register.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\LoginForm\\LoginForm.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\Home\\Home.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\Forgot\\Forgot.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\Reset\\Reset.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\User\\User.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\Header\\Header.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\components\\UserChange\\UserChange.jsx",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\redux\\store.js",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\redux\\reducers\\index.js",[],"C:\\Users\\olysi\\OneDrive\\Рабочий стол\\My Projects\\AuthApp\\authapp\\client\\src\\redux\\redu